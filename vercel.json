{
    "version": 2,
    "builds": [
        {
            "src": "public",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/public/(.*)",
            "dest": "/public/$1"
        },
        {
            "src": "/(.*)",
            "dest": "server.js"
        }
    ]
}